{
  "name": "family-retail-predictor-backend",
  "version": "1.0.0",
  "description": "Backend API for Family Retail Predictor - A full-stack retail shopping application",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "build": "npm run frontend:build",
    "install:all": "npm install && npm run frontend:install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "retail",
    "shopping",
    "family",
    "predictor",
    "express",
    "mongodb",
    "nodejs",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
